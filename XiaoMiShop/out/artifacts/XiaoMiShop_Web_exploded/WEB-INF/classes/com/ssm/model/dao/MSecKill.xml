<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssm.model.dao.SecKillDAO">

    <insert id="addSecKill"  parameterType="SecKillInfo">
    	insert into seckill(goods_detail_id,seckill_num,seckill_starttime,seckill_endtime,create_time,seckill_status,seckill_money)
    	values(#{goods_detail_id},#{seckill_num},#{seckill_starttime},#{seckill_endtime},#{create_time},#{seckill_status},#{seckill_money})
    </insert>
    
</mapper>
